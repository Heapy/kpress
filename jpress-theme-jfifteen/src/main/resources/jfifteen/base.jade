	body(class='home blog custom-background')
		div#page.hfeed.site
			a.skip-link.screen-reader-text(href='#content') Skip to content

			div#sidebar.sidebar
				header#masthead.site-header(role='banner')
					div.site-branding
						h1.site-title
							a(href='./', rel='home') Izotova Natalie
						p.site-description Best Photographer Ever
						button.secondary-toggle Menu and widgets
				block sidebar
					div#secondary.secondary
						nav#social-navigation.social-navigation(role='navigation')
							div.menu-social-container
								ul#menu-social.menu
									li(class='menu-item menu-item-type-custom menu-item-object-custom menu-item-1164')
										a(href="http://linkedin.com/pub/ruslan-ibragimov/68/791/20/")
											span(class="screen-reader-text") LinkedIn
									li(class='menu-item menu-item-type-custom menu-item-object-custom menu-item-1165')
										a(href="https://github.com/IRus")
											span(class="screen-reader-text") GitHub
									li(class='menu-item menu-item-type-custom menu-item-object-custom menu-item-1167')
										a(href="http://ruslan.ibragimov.by/whoami")
											span(class="screen-reader-text") I

			div#content.site-content
				block content

			block footer	
				footer#colophon.site-footer(role='contentinfo')
					div.site-info
						a(href='https://wordpress.org/') Proudly powered by WordPress

		block template-footer
			script(type='text/javascript', src='./js/skip-link-focus-fix.js?ver=20141010')
