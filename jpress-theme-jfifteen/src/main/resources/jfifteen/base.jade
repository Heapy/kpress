		script (function(){document.documentElement.className='js'})();
		block title
			title Izotova Art
		block template-head
			link#genericons-css(rel='stylesheet', href='./genericons/genericons.css?ver=3.2', type='text/css', media='all')
			link#twentyfifteen-style-css(rel='stylesheet',  href='./style.css?ver=4.1.1', type='text/css', media='all')
